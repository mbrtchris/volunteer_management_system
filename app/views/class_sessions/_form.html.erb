<%= simple_form_for(@class_session) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :subject %>
    <%= f.input :notes %>
    <%= f.input :number_of_students %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :room %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
