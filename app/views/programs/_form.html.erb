<%= simple_form_for(@program) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.association :profile %>
    <%= f.association :class_session %>
    <%= f.association :training_record %>
    <%= f.association :training %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
