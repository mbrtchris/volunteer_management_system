<h1>Listing training_records</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Type</th>
      <th>Training</th>
      <th>Has passed</th>
      <th>Expires at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @training_records.each do |training_record| %>
      <tr>
        <td><%= training_record.profile %></td>
        <td><%= training_record.type %></td>
        <td><%= training_record.training %></td>
        <td><%= training_record.has_passed %></td>
        <td><%= training_record.expires_at %></td>
        <td><%= link_to 'Show', training_record %></td>
        <td><%= link_to 'Edit', edit_training_record_path(training_record) %></td>
        <td><%= link_to 'Destroy', training_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Training record', new_training_record_path %>
